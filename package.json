{
  "name": "qnap-college",
  "version": "1.0.0",
  "private": true,
  "description": "Example project from an angular.io guide.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --host 0.0.0.0",
    "test": "ng test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "node --max_old_space_size=8192 'node_modules/@angular/cli/bin/ng' build --prod --aot && ng run qnap-college:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "deploy:production": "pm2 deploy production setup; pm2 deploy production",
    "deploy:staging": "pm2 deploy staging setup; pm2 deploy staging",
    "postinstall": "npm run build:ssr",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "generate:prerender": "cd dist && node prerender",
    "compile:server": "tsc -p server.tsconfig.json"
  },
